<mvc:View controllerName="gopro1.project1.controller.GOPRO"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m">
   	<List
		headerText="Products"
		headerLevel="H2"
		items="{
			path: 'products>/'
		}" >
		<StandardListItem
			title="{products>productName}"
			description="{products>productCategory}"/>
	</List>       
   
</mvc:View>
